  <data name="QueryBaseBusqueda" xml:space="preserve">
    <value>SET LANGUAGE SPANISH

SELECT GC_Remitente AS Remitente
	,convert(VARCHAR, GC_Fecha, 103) + ' ' + convert(VARCHAR, GC_Fecha, 108) AS Fecha
	,GC_Asunto AS Asunto
	,C.GC_Id AS ID
	,GC_Cuerpo AS Correo
FROM GlosarioCorreos C
</value>
  </data>
  <data name="QueryInfoCorreo" xml:space="preserve">
    <value>DECLARE @Destinatario VARCHAR(150)
DECLARE @Destinatarios VARCHAR(4000)

SET @Destinatarios = ''

DECLARE cursorBloq CURSOR FAST_FORWARD FOR
  SELECT DISTINCT GCD_Destinatario
  FROM   GlosarioCorreos_Destinatarios
  WHERE GC_Id = @@GC_Id
  ORDER  BY 1
  FOR READ ONLY

OPEN cursorBloq

FETCH NEXT FROM cursorBloq INTO @Destinatario

WHILE @@FETCH_STATUS = 0
  BEGIN
      SET @Destinatarios = @Destinatarios + @Destinatario + '; '

      FETCH NEXT FROM cursorBloq INTO @Destinatario
  END

CLOSE cursorBloq

DEALLOCATE cursorBloq

SELECT 'Valor' AS Concepto
	,C.GC_Remitente AS Remitente
	,@Destinatarios AS Destinatarios
	,CONVERT(VARCHAR, C.GC_Fecha, 103) + ' ' + CONVERT(VARCHAR, C.GC_Fecha, 108) AS Fecha
	,C.GC_Asunto AS Asunto
	,C.GC_Ruta AS Carpeta
	,K.GCP_Equipo AS Equipo
	,K.GCP_Ruta AS Ruta
	,K.GCP_Archivo AS Archivo
FROM dbo.GlosarioCorreos C
JOIN GlosarioCorreos_Catalogo K ON C.GCP_Id = K.GCP_Id
WHERE C.GC_Id = @@GC_Id</value>
  </data>
